{{#each this}}
<li class="county-item">
<h1 class="country-name">{{name}}</h1>
<div class="country-info">
    <p> Capital:{{capital}}</p>
    <p>Population:{{population}}</p>
    <ul>
        <p>Languages:</p>
        {{#each languages}}
        <li>{{this.name}}</li>
        {{/each}}
    </ul>
</div>
<div class="flag-wrapper">
    <img src="{{flag}}" alt="flag" class="img-flag" width="400" height="auto">
</div>

</li>

{{/each}}



